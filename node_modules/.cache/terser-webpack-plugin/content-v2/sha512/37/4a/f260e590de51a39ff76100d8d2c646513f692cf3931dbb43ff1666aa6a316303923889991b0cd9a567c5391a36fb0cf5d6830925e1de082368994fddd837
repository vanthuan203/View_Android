eed,\n        comment_rate,\n        mobile_rate,\n        search_rate,\n        enabled,\n        max_thread,\n        group_id,\n        like_rate,\n        comment_list,\n        suggest_rate,\n        premium_rate,\n        keyword,\n        view_percent,\n      })\n      if (resp && resp.data) {\n        const data = resp.data\n        if (data.status === 'fail') {\n          setStatus(STATUS_ERROR)\n          setErrorMsg(data.message)\n          onStatusChange && onStatusChange(STATUS_ERROR)\n        } else if (data.status === 'success') {\n          setStatus(STATUS_SUCCESSFUL)\n          onStatusChange && onStatusChange(STATUS_SUCCESSFUL)\n        }\n      }\n\n      console.log('processChannel', resp)\n    } catch (e: any) {\n      console.log('processChannel', e)\n      setStatus(STATUS_ERROR)\n      setErrorMsg(e.message)\n      onStatusChange && onStatusChange(STATUS_ERROR)\n    }\n  }\n\n  return (\n    <>\n      <div className='row'>\n        <div className='col-1 d-flex align-items-center'>{index + 1}.</div>\n        <div className='col-7 d-flex  align-items-center'>\n          <span>{channel}</span>\n        </div>\n        <div className='col-4 d-flex justify-content-end align-items-center'>\n          {status === STATUS_ERROR ? (\n            <>\n              <div className='d-flex flex-row align-items-center'>\n                <Tooltip title={errorMsg}>\n                  <span id={`error_${index}`}>\n                    <i className='bi bi-info-circle-fill text-danger'></i>\n                  </span>\n                </Tooltip>\n                <Button\n                  type='link'\n                  onClick={() => {\n                    setStatus(STATUS_PROCESSING)\n                    setErrorMsg('')\n                  }}\n                >\n                  Thử lại\n                </Button>\n              </div>\n            </>\n          ) : status === STATUS_SUCCESSFUL ? (\n            <span id={`error_${index}`}>\n              <i className='bi bi-check-circle-fill text-success'></i>\n            </span>\n          ) : (\n            <Spinner size='sm' />\n          )}\n        </div>\n      </div>\n    </>\n  )\n}\n\nexport default ChannelItem\n","import 'bootstrap/dist/css/bootstrap.min.css'\n\nimport {Form, FormGroup, Input, Label, Modal} from 'reactstrap'\nimport {KTSVG, toAbsoluteUrl} from '_metronic/helpers'\nimport React, {useEffect, useState} from 'react'\nimport {shallowEqual, useSelector} from 'react-redux'\n\nimport ChannelItem from './ChannelItem'\nimport {Group} from '../../models/Order'\nimport {RootState} from 'setup'\nimport {actions} from '../../redux/OrdersRedux'\nimport {useDispatch} from 'react-redux'\n\ntype Props = {\n  show: boolean\n  close: () => void\n}\n\nconst STEP_INIT = 0\nconst STEP_PROCESSING = 1\n\nconst AddModal: React.FC<Props> = ({show, close}) => {\n  const role: string =\n    (useSelector<RootState>(({auth}) => auth.user?.role, shallowEqual) as string) || ''\n  const balance: number =\n    (useSelector<RootState>(({auth}) => auth.user?.balance, shallowEqual) as number) || 0\n  const groups: Group[] =\n    (useSelector<RootState>(({orders}) => orders.groups, shallowEqual) as Group[]) || []\n\n  const dispatch = useDispatch()\n  const [adding, setAdding] = useState(false)\n  const [max_thread, setMax_thread] = useState(5)\n  const [channel_id, setChannel_id] = useState('')\n  //\n  const [view_need, setView_need] = useState(1000)\n  const [premium_rate, setPremium_rate] = useState(5)\n  const [view_percent, setView_percent] = useState(30)\n  //\n  const [home_rate, setHome_rate] = useState(25)\n  const [search_rate, setSearch_rate] = useState(25)\n  const [suggest_rate, setSuggest_rate] = useState(25)\n  const [direct_rate, setDirect_rate] = useState(25)\n  //\n  const [like_rate, setLike_rate] = useState(25)\n  const [comment_rate, setComment_rate] = useState(25)\n  const [mobile_rate, setMobile_rate] = useState(0)\n  //\n  const [note, setNote] = useState('')\n  const [keyword, setKeyword] = useState('')\n  const [comment_list, setComment_list] = useState('')\n  const [group_id, setGroup] = useState(0)\n  //\n  const [enabled, setEnabled] = useState(1)\n\n  const [step, setStep] = useState(STEP_INIT)\n\n  const dismissModal = () => {\n    dispatch(actions.clearcurrentOrder())\n  }\n\n  const clearData = () => {\n    // setStep(STEP_INIT);\n    // setAdding(false);\n    // setMax_thread(5);\n    // setChannel_id('');\n    // setView_need(1000);\n    // setPremium_rate(5);\n  }\n\n  const submit = () => {\n    if (view_need % 100 !== 0 || view_need == 0) {\n      alert('Số sub cần chạy phải chia hết cho 100')\n      return\n    }\n    if (channel_id.trim() === '') {\n      alert('Điền danh sách kênh cần chạy')\n      return\n    }\n    if (home_rate + search_rate + suggest_rate + direct_rate != 100) {\n      alert('Tổng nguồn view không đúng')\n      return\n    }\n    if (like_rate > 100 || like_rate < 0) {\n      alert('Tỉ lệ like không đúng')\n      return\n    }\n    if (view_percent > 100 || view_percent < 0) {\n      alert('phần trăm xem video không đúng')\n      return\n    }\n    if (comment_rate > 100 || comment_rate < 0) {\n      alert('Tỉ lệ comment không đúng')\n      return\n    }\n    setAdding(true)\n    setStep(STEP_PROCESSING)\n    return\n  }\n\n  const processing = step === STEP_PROCESSING\n\n  console.log('processing', processing)\n\n  const _renderChannelInput = () => {\n    return (\n      <Input\n        id='list_id'\n        name='list_id'\n        className='form-control form-control-solid'\n        placeholder={role === 'ROLE_ADMIN' ? 'Mỗi id 1 dòng' : 'Id kênh'}\n        value={channel_id}\n        type={role === 'ROLE_ADMIN' ? 'textarea' : 'text'}\n        onChange={(e) => setChannel_id(e.target.value)}\n        style={{height: '200px'}}\n      />\n    )\n  }\n\n  const _renderChannelProcessItem = () => {\n    const arrChannel = channel_id.split('\\n')\n    console.log('_renderChannelProcessItem', arrChannel)\n    if (arrChannel.length) {\n      return arrChannel.map((channel, index) => {\n        if (channel)\n          return (\n            <li className='list-group-item' key={`channel_${index}`}>\n              <ChannelItem\n                {...{\n                  channel,\n                  home_rate,\n                  note,\n                  direct_rate,\n                  view_need,\n                  comment_rate,\n                  mobile_rate,\n                  search_rate,\n                  enabled,\n                  max_thread,\n                  group_id,\n                  like_rate,\n                  comment_list,\n                  suggest_rate,\n                  premium_rate,\n                  keyword,\n                  view_percent,\n                }}\n                index={index}\n              />\n            </li>\n          )\n      })\n    }\n    return ''\n  }\n\n  return (\n    <Modal\n      unmountOnClose\n      size='xl'\n      isOpen={show}\n      modalTransition={{timeout: 500}}\n      onClosed={clearData}\n    >\n      <div className='modal-header'>\n        <h5 className='modal-title'>Thêm nhiệm vụ</h5>\n        <div className='btn btn-icon btn-sm btn-active-light-primary ms-2' aria-label='Close'>\n          <span className='svg-icon svg-icon-2x'></span>\n        </div>\n      </div>\n      <div className='modal-body'>\n        <p>Cài đặt danh sách đơn</p>\n        <Form>\n          <div className='row'>\n            <div className='col-6'>\n              <FormGroup>\n                <Label for='exampleEmail' className='required form-label'>\n                  Danh sách id kênh cần chạy sub\n                </Label>\n                {step === STEP_INIT ? (\n                  _renderChannelInput()\n                ) : (\n                  <div className='card border'>\n                    <div className='card-body'>\n                      <ul className='list-group list-group-flush'>{_renderChannelProcessItem()}</ul